{
    "manifest_version": 3,
    "name": "Leetcode Recall",
    "description": "leetcode recall workflow assistant",
    "version": "1.0",
    "permissions": ["storage"],
    "host_permissions": ["https://leetcode.com/*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["https://leetcode.com/problems/*"],
        "js": ["content.js"]
        }
    ]
}